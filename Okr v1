local vim = game:GetService("VirtualInputManager")

-- Create UI
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = game.Players.LocalPlayer.PlayerGui

-- Create Parry Button
local parryButton = Instance.new("TextButton")
parryButton.Size = UDim2.new(0, 200, 0, 50)
parryButton.Position = UDim2.new(0.5, -100, 0.9, -25)
parryButton.Text = "Parry"
parryButton.Parent = screenGui

local isParrying = false

parryButton.MouseButton1Click:Connect(function()
    if isParrying then
        isParrying = false
        parryButton.Text = "Parry"
    else
        isParrying = true
        parryButton.Text = "Stop"
        while isParrying do
            vim:SendKeyEvent(true, "F", false, game)
            vim:SendKeyEvent(false, "F", false, game)
            wait(0.1)
        end
    end
end)
